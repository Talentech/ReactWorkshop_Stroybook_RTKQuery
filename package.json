{
  "name": "react-hrid",
  "version": "1.0.1",
  "description": "Starterpack of ReactJS app authenticated via HRID",
  "main": "build/app.js",
  "scripts": {
    "test": "jest --maxWorkers=1",
    "majestic": "npx majestic",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "webpack serve --mode=development",
    "build:prod": "rimraf build && npm run lint && npm run test && cross-env NODE_ENV=production webpack --progress --mode=production",
    "build:docker": "rimraf build_docker && npm run lint && npm run test && cross-env NODE_ENV=docker webpack --progress --mode=production",
    "build:test": "rimraf build && npm run lint && npm run test && cross-env NODE_ENV=test webpack --progress --optimize-minimize",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.ts,.tsx --fix"
  },
  "author": "Mateusz Lewandowski",
